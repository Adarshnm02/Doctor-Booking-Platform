{
  "compilerOptions": {
    "target": "es2016",
    "module": "NodeNext",                 // Use NodeNext here instead of ESNext
    "moduleResolution": "nodenext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["vite/client"]
  }
}
